service zrm-palomino
{
        type            = UNLISTED
        disable         = no
        socket_type     = stream
        protocol        = tcp
        wait            = no
        user            = mysql
        group           = mysql
        instances       = 2
        server          = /usr/share/mysql-zrm/plugins/socket-server.palomino.pl
        port            = 25300
}

